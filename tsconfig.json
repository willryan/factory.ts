{
  "include": ["**/*.ts"],
  "compilerOptions": {
    "module": "commonjs",
    "target": "esNEXT",
    "lib": ["esNEXT"],
    "alwaysStrict": true,
    "strictFunctionTypes": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "inlineSourceMap": true,
    "strictNullChecks": true,
    "baseUrl": "."
  },
}

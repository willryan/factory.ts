{
  "name": "factory.ts",
  "version": "0.3.4",
  "license": "MIT",
  "description": "A Typescript test data factory similar to factory_bot and rosie",
  "scripts": {
    "build": "tsc -p .",
    "test": "jest --colors",
    "test:debug": "node --inspect=9229 --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:watch": "yarn test:truncate && jest --watch"
  },
  "jest": {
    "reporters": ["default"],
    "transform": {
      "\\.(js|ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "./spec/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.ts"
  },
  "repository": "https://github.com/willryan/factory.ts",
  "engines": {
    "node": ">= 0.10.0"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "author": "Will Pleasant-Ryan",
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "@types/node": "^7.0.4",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "jest": "^20.0.0",
    "jest-environment-node-debug": "^2.0.0",
    "jsdom": "^9.8.3",
    "ts-jest": "^20.0.7",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "typescript": "3.2.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "source-map-support": "^0.5.9",
    "tslint": "^5.11.0"
  }
}
{
  "name": "factory.ts",
  "version": "0.5.1",
  "license": "MIT",
  "description": "A Typescript test data factory similar to factory_bot and rosie",
  "scripts": {
    "build": "rimraf lib && tsc -p ./tsconfig.build.json",
    "test": "jest --colors",
    "test:debug": "node --inspect=9229 --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "build-and-publish": "yarn build && npm login && npm publish",
    "check:script": "eslint --fix-dry-run 'src/**/*.ts'",
    "lint:script": "eslint --fix 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "jest": {
    "reporters": [
      "default"
    ],
    "transform": {
      "\\.(js|ts)$": "ts-jest"
    },
    "testRegex": "./spec/.*\\.spec\\.ts$",
    "moduleFileExte nsions": [
      "js",
      "ts",
      "json"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ]
  },
  "repository": "https://github.com/willryan/factory.ts",
  "engines": {
    "node": ">= 0.10.0"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "author": "Will Pleasant-Ryan",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": ">=6",
    "jest": "^26.6.3",
    "jest-environment-node-debug": "^2.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "clone-deep": "^4.0.1",
    "source-map-support": "^0.5.19"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*.{js,json,md}": "prettier --write"
  }
}
